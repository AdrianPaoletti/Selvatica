{
  "extends": ["eslint:recommended", "next/core-web-vitals", "prettier"],
  "plugins": ["react", "@typescript-eslint", "testing-library"],
  "rules": {
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-restricted-syntax": "off",
    "consistent-return": "off",
    "no-debugger": "off",
    "no-console": "off"
  }
}
